footer a:hover.linkedin { background: #1b86bd; }
footer a:hover.facebook { background: #1b60bd; }
footer a:hover.instagram { background: #3d82a6; }
footer a:hover.pinterest { background: #c23434; }
footer a:hover.twitter { background: #5ecae1; }

.copyright { padding: 40px 0; width: 100%; text-align: center; }
.copyright p { color: #949493; font-size: 12px; }
.copyright a { color: #ff4c00; }

#cboxContent .textfeatures { padding: 40px; }
#cboxContent .textfeatures h1 { font-size: 25px; }
#cboxContent .textfeatures p { font-size: 16px; color: #333; line-height: 1.4; margin-bottom: 15px; }
#cboxContent .textfeatures ul { margin-left: 10px; font-size: 16px; list-style: disc; }

