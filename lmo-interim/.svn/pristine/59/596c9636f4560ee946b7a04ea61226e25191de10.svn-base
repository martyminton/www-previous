section#contact .form-container .col-1 .inputWrapper label.error { position: absolute; top: 5px; right: 0; font-family: "Gotham SSm A", "Gotham SSm B"; font-size: 14px; color: red; }
section#contact .form-container .col-1 input, section#contact .form-container .col-1 textarea { margin-bottom: 40px; }
section#contact .form-container .col-1 textarea { height: 153px; }
section#contact .form-container .col-1 span.sub-message { margin-top: -30px; display: block; color: #949493; }

footer { width: 100%; height: 100px; background: #646469; position: relative; }
footer a { width: 20%; display: block; text-align: center; padding-top: 30px; height: 70px; float: left; background: none; -moz-transition: background, 0.3s; -o-transition: background, 0.3s; -ms-transition: background, 0.3s; -webkit-transition: background, 0.3s; transition: background, 0.3s; }
